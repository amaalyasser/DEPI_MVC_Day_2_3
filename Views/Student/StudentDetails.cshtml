<div class="card" style="width: 18rem;">
    <img class="card-img-top" src="/Images/@Model.Img" alt="Card image cap">
    <div class="card-body">
        <h5 class="card-title">@Model.Id</h5>
        <p class="card-text">@Model.Name</p>
        <p class="card-text">@Model.Address</p>
        <a href="@Url.Action("Index", "Student")" class="btn btn-primary">back</a>
    </div>
</div>